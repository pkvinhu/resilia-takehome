<div id="notifications">
  <a class="not_submit" href="#" onclick="getNotifications()"
    >Get New Notifications</a
  >
  <a class="not_submit" href="#" onclick="clearNotifications()"
    >Clear Notifications</a
  >

  <% if (data.message) { %>
  <div id="notifications-section">
    <div id="notifications-container">
      <% if (data.notifications.length) { %>
      <h3>Notifications</h3>
      <hr />
      <% data.notifications.forEach((notification, i) => { %>
      <div class="notification"><%= i+1%> <%= notification %></div>
      <% }); %> <% } else { %>
      <div>No Notifications!</div>
      <% } %>
    </div>
  </div>
  <% } %>
</div>
